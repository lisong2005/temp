<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd"
	default-autowire="byName">



	<!-- OpenTokenRepository -->
	<bean id="openTokenRepository"
		class="com.witon.ehealth.core.model.wx.impl.OpenTokenRepositoryImpl" />

	<!-- WeixinEventRepository -->
	<bean id="weixinEventRepository"
		class="com.witon.ehealth.core.model.wx.impl.WeixinEventRepositoryImpl" />

	<!-- WeixinUserRepository -->
	<bean id="weixinUserRepository"
		class="com.witon.ehealth.core.model.wx.impl.WeixinUserRepositoryImpl" />

	<!-- WxAppConfigRepository -->
	<bean id="wxAppConfigRepository"
		class="com.witon.ehealth.core.model.wx.impl.WxAppConfigRepositoryImpl" />



</beans>
